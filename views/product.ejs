<!-- products.ejs -->

<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Products</title>
  </head>
  <body>
    <h1>Products</h1>
    <ul>
      <% for (const product of products) { %>
        <li><%= product.name %> - <%= product.price %></li>
      <% } %>
    </ul>
  </body>
</html>
